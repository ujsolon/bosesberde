{
  "deployment": {
    "region": "us-west-2",
    "stage": "prod",
    "servers": {
      "aws-documentation": {
        "enabled": true,
        "stack_name": "mcp-aws-documentation-server",
        "description": "AWS Documentation Search MCP Server"
      },
      "aws-pricing": {
        "enabled": true,
        "stack_name": "mcp-aws-pricing-server",
        "description": "AWS Pricing Information MCP Server"
      },
      "bedrock-kb-retrieval": {
        "enabled": true,
        "stack_name": "mcp-bedrock-kb-retrieval-server",
        "description": "Amazon Bedrock Knowledge Base Retrieval MCP Server"
      },
      "tavily-web-search": {
        "enabled": true,
        "stack_name": "mcp-tavily-web-search-server",
        "description": "Tavily Web Search MCP Server"
      },
      "financial-market": {
        "enabled": true,
        "stack_name": "mcp-financial-analysis-server",
        "description": "Financial Market Data and Analysis MCP Server"
      },
      "recruiter-insights": {
        "enabled": true,
        "stack_name": "mcp-recruiter-insights-server",
        "description": "Recruiter Insights and Candidate Analysis MCP Server"
      }
    }
  },
  "environment_variables": {
    "aws-documentation": {
      "LOG_LEVEL": "INFO",
      "MAX_RESULTS": "20"
    },
    "aws-pricing": {
      "LOG_LEVEL": "INFO",
      "CACHE_TTL": "3600"
    },
    "bedrock-kb-retrieval": {
      "LOG_LEVEL": "INFO",
      "BEDROCK_REGION": "us-west-2",
      "KNOWLEDGE_BASE_ID": "{KB_ID}"
    },
    "tavily-web-search": {
      "LOG_LEVEL": "INFO",
      "TAVILY_API_KEY": "{TAVILY_API_KEY}"
    },
    "financial-market": {
      "LOG_LEVEL": "INFO"
    },
    "recruiter-insights": {
      "LOG_LEVEL": "INFO",
      "S3_BUCKET_NAME": "recruiter-insights-data"
    }
  },
  "testing": {
    "enabled": true,
    "timeout": 30,
    "test_cases": {
      "aws-documentation": [
        {
          "name": "Search AWS Lambda documentation",
          "method": "searchDocumentation",
          "arguments": {
            "search_phrase": "AWS Lambda function",
            "limit": 5
          }
        }
      ],
      "aws-pricing": [
        {
          "name": "Get pricing service codes",
          "method": "getPricingServiceCodes",
          "arguments": {}
        }
      ],
      "bedrock-kb-retrieval": [
        {
          "name": "List knowledge bases",
          "method": "listKnowledgeBases",
          "arguments": {}
        }
      ],
      "tavily-web-search": [
        {
          "name": "Search web content",
          "method": "searchWeb",
          "arguments": {
            "query": "AWS Lambda best practices",
            "max_results": 3
          }
        }
      ],
      "financial-market": [
        {
          "name": "Get stock quote",
          "method": "stock_quote",
          "arguments": {
            "symbol": "AAPL"
          }
        },
        {
          "name": "Get market data",
          "method": "market_data",
          "arguments": {}
        }
      ],
      "recruiter-insights": [
        {
          "name": "List S3 bucket contents",
          "method": "list_s3_bucket",
          "arguments": {
            "prefix": ""
          }
        },
        {
          "name": "Extract resume data",
          "method": "extract_resume_data",
          "arguments": {}
        }
      ]
    }
  }
}
